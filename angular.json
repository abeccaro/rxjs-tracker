{
  "$schema": "lib/node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "lib": {
      "projectType": "library",
      "root": "lib",
      "sourceRoot": "lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "lib/ng-package.json"
          },
          "configurations": {}
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "lib/src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "lib/karma.conf.js"
          }
        }
      }
    }},
  "defaultProject": "lib"
}
